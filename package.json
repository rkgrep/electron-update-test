{
  "name": "electron-experiment",
  "version": "1.0.1",
  "description": "",
  "private": true,
  "author": "",
  "license": "ISC",
  "repository": "https://github.com/rkgrep/electron-update-test",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "release": "cross-env NODE_ENV=production node .electron-vue/build.js && electron-builder --publish=always",
    "postinstall": ""
  },
  "browserslist": [
    "chrome 71"
  ],
  "build": {
    "productName": "Electron Experiment",
    "appId": "pro.hackforge.electron-exp",
    "copyright": "Copyright Â© 2020 Hackforge",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "afterSign": ".electron-vue/notarize.js",
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/electron_icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "nsisWeb": {
      "deleteAppDataOnUninstall": true,
      "allowElevation": false
    },
    "win": {
      "publisherName": "Red Market LLC",
      "icon": "build/icons/electron_icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@firebase/app": "^0.5.0",
    "@firebase/auth": "^0.13.3",
    "@firebase/storage": "^0.3.22",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@pusher/chatkit-client": "1.13.3",
    "@sentry/electron": "^1.1.0",
    "axios": "^0.19.0",
    "axios-logger": "^2.2.1",
    "better-queue": "^3.8.10",
    "better-queue-memory": "^1.0.4",
    "bootstrap-scss": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "browser-env": "^3.3.0",
    "conf": "6.2.0",
    "croppr": "^2.3.1",
    "deepmerge": "^4.2.2",
    "electron-dl": "^2.0.0",
    "electron-online": "^1.0.0",
    "electron-promise-ipc": "^2.0.1",
    "electron-store": "^5.1.0",
    "electron-updater": "^4.2.0",
    "jquery": "^3.4.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "luxon": "^1.21.3",
    "mime-types": "^2.1.25",
    "offline-js": "^0.7.19",
    "pusher-js": "^5.0.3",
    "semver": "^7.1.0",
    "source-map-support": "^0.5.16",
    "uuid": "^3.3.3",
    "vue": "^2.6.11",
    "vue-datetime": "^1.0.0-beta.10",
    "vue-electron": "^1.0.6",
    "vue-loading-overlay": "^3.2.0",
    "vue-multipane": "^0.9.5",
    "vue-router": "^3.1.3",
    "vuebar": "^0.0.20",
    "vuex": "^3.1.2",
    "vuex-electron": "^1.0.3",
    "vuex-router-sync": "^5.0.0",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.7.6",
    "ajv": "^6.10.2",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.4.5",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "dotenv": "^8.2.0",
    "electron": "7.1.2",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.2.1",
    "envalid": "^6.0.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "url-loader": "^3.0.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
